{
  "name": "react-express-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.8.1",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.18.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "4.10.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "object-assign": "4.1.1",
    "passport": "^0.4.0",
    "passport-google-token": "^0.1.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "query-string": "^6.1.0",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.2",
    "react-router": "3.2.0",
    "request": "^2.88.0",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "superagent": "^3.8.3",
    "supertest": "^3.1.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon server/index.js --watch",
    "dev:build-client": "node client/scripts/start.js",
    "build": "CI=false node client/scripts/build.js",
    "test": "CI=false node client/scripts/test.js --env=jsdom",
    "test:watch": "node client/scripts/test.js --env=jsdom --watch",
    "test:coverage": "node client/scripts/test.js --env=jsdom --coverage",
    "eslint:fix": "eslint --ignore-path .gitignore --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run eslint:fix",
      "git add"
    ],
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx,mjs}",
      "server/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/client/config/polyfills.js",
      "<rootDir>/client/config/jest/jestSetup.js"
    ],
    "testMatch": [
      "<rootDir>/client/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/client/src/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/server/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/server/**/__tests__/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/client/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/client/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "@helpers(.*)$": "<rootDir>/client/src/helpers/$1",
      "@constants(.*)$": "<rootDir>/client/src/constants/$1",
      "@components(.*)$": "<rootDir>/client/src/components/$1",
      "@pages(.*)$": "<rootDir>/client/src/pages/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {
    "babel-plugin-import": "^1.8.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-jarvis": "^0.3.2"
  }
}
